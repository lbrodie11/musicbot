{
  "name": "musicbot",
  "version": "1.0.0-Beta",
  "main": "main.js",
  "scripts": {
    "start": "node main.js start",
    "dev": "nodemon main.js start --schedule \"1 * * * * *\"",
    "dev:server": "nodemon main.js start --schedule \"* * * 31 2 *\"",
    "debug": "env DEBUG=\"*\" nodemon --inspect main.js start --schedule \"0 * * * * *\" --debug",
    "format": "prettier {src,public}/**/*.{js,ts,json,md,scss} --write",
    "format:check": "prettier {src,public}/**/*.{js,ts,json,md,scss} --list-different",
    "test:server": "node main.js start --schedule \"* * * 31 2 *\""
  },
  "esm": {
    "mode": "auto",
    "cjs": true
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.3",
    "dotenv": "^6.0.0",
    "esm": "^3.0.52",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "http-debug": "^0.1.2",
    "log4js": "^2.9.0",
    "method-override": "^2.3.10",
    "mongoose": "^5.1.6",
    "mongoose-findorcreate": "^3.0.0",
    "node-fetch": "^2.1.2",
    "node-schedule": "^1.3.0",
    "now-env": "^3.0.4",
    "oauth": "^0.9.15",
    "passport": "^0.4.0",
    "passport-spotify": "^1.0.0",
    "puppeteer": "^1.5.0",
    "query-params": "^0.0.1",
    "spotify-web-api-node": "^3.1.1",
    "yargs": "^11.0.0"
  },
  "devDependencies": {
    "imagemin": "^5.3.1",
    "imagemin-pngquant": "^5.1.0",
    "nodemon": "^1.17.5",
    "prettier": "^1.12.1",
    "standard-version": "^4.3.0"
  },
  "description": "Music Lackey - Release of new albums by music artists",
  "author": "Laqwan Brodie",
  "license": "ISC"
}
